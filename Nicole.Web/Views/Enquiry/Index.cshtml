@Scripts.Render("~/bundles/EnquiryManager")
<div class="row">
    <div class="col-md-12">
        <div class="navbar-form navbar-right">
            <a class="btn btn-default" data-bind="click:EnquiryManager.viewModel.ShowSearch">搜索</a>
            <a class="btn btn-default" data-bind="click:EnquiryManager.viewModel.Print">打印</a>
            <a class="btn  btn-primary" data-bind="click:EnquiryManager.viewModel.ShowCreate">新增</a>
        </div>
    </div>
</div>

<table class="table table-responsive">
    <thead>
        <tr>
            <th>客户</th>
            <th>料号</th>
            <th>型号</th>
            <th>电压</th>
            <th>容量</th>
            <th>脚距</th>
            <th>等级</th>
            <th>特殊设计</th>
            <th>报价</th>
            <th>报价日期</th>
            
        </tr>
    </thead>
    <tbody data-bind="foreach:viewModel.Page.EnquiryModels">
        <tr>
            <td data-bind="text:CustomerModel.Name"></td>
            <td data-bind="text:PartNumber"></td>
            <td data-bind="text:ProductType"></td>
            <td data-bind="text:Voltage"></td>
            <td data-bind="text:Capacity"></td>
            <td data-bind="text:Pitch"></td>
            <td data-bind="text:Level"></td>
            <td>
                <a href="#" data-bind="click:EnquiryManager.viewModel.ShowSpecificDesign,visible:SpecificDesign()!=null">查看</a>
            </td>
            <td data-bind="text:Price"></td>
            <td data-bind="time:CreatedTime"></td>
        </tr>
    </tbody>
</table>
<div id="page-selection" class="text-center"></div>
<!-- /新增 -->
<div class="modal fade" id="createdialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">新增</h4>
            </div>
            <div class="modal-body">
                <div class="formbox" style="margin: auto">
                    <div class="form-group">
                        <input type="text" class="form-control" placeholder="料号" data-bind="value:EnquiryManager.viewModel.EnquiryModel.ProductModel.PartNumber">
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" placeholder="客户" data-bind="value:EnquiryManager.viewModel.EnquiryModel.CustomerModel.Code">
                    </div>                   
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-bind="click:EnquiryManager.viewModel.CreateSave">保存</button>
                <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
            </div>
        </div>
    </div>
</div>

<!-- /搜索 -->
<div class="modal fade" id="searchdialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">搜索</h4>
            </div>
            <div class="modal-body">
                <div class="formbox" style="margin: auto">
                    <div class="form-group">
                        <input type="text" class="form-control" placeholder="客户关键字" id="CustomerNameKey">
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" placeholder="料号关键字" id="PartNumberKey">
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" placeholder="型号关键字" id="ProductTypeKey">
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" placeholder="电压关键字" id="VoltageKey">
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" placeholder="容量关键字" id="CapacityKey">
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" placeholder="脚距关键字" id="PitchKey">
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" placeholder="等级关键字" id="LevelKey">
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" placeholder="特殊设计关键字" id="SpecificDesignKey">
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-bind="click:EnquiryManager.viewModel.Search">确定</button>
                <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
            </div>
        </div>
    </div>
</div>